# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /areas/ids                             com.github.morikuni.locest.area.application.controller.Application.areas_ids
GET     /areas/ids/coordinate/:lat/:lng        com.github.morikuni.locest.area.application.controller.Application.areas_ids_coordinate(lat: Double, lng: Double)
GET     /areas/:id                             com.github.morikuni.locest.area.application.controller.Application.areas(id: Int)